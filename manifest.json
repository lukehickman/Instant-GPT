{
  "manifest_version": 3,
  "name": "Instant GPT",
  "version": "1.2",
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      }
    }
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_security_policy": {}
}